<tr id="movie-<%= movie.id %>">
  <td><%= movie.title %></td>
  <td><%= movie.m_format %></td>
  <td><%= movie.length_hours %></td>
  <td><%= movie.length_minutes %></td>
  <td><%= movie.release_year %></td>
  <td><%= movie.rating %></td>
  <td colspan="3">
    <%= link_to 'Edit', movies_path(id: movie) %> |
    <%= link_to 'Destroy', movie_path(movie), method: :delete, data: { remote: true, confirm: 'Are you sure?' } %>
  </td>
</tr>